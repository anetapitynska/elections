<div class="voivodeship_wrapper">
<h1> Wszystkie głosy </h1>

</div>


<div id="page_wrapper">


  <table class="table table-striped">
  <thead>
    <tr>
      <th>Ilość głosów</th>
          <th>Okręg wyborczy</th>
              <th>Komitet wyborczy</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.number %></td>
        <td><%= vote.area.name%></td>
        <td><%= vote.commitee.name %></td>
        <td><%= link_to 'Pokaż', vote %></td>
        <td><%= link_to 'Edytuj', edit_vote_path(vote) %></td>
        <td><%= link_to 'Usuń', vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>





<br>

</div>