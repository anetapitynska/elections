<div id="page_wrapper">
<h1>All Votes</h1>

  <table class="table table-striped">
  <thead>
    <tr>
      <th>Number</th>
          <th>Area</th>
              <th>Commitee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.number %></td>
        <td><%= vote.area.name%></td>
        <td><%= vote.commitee.name %></td>
        <td><%= link_to 'Show', vote %></td>
        <td><%= link_to 'Edit', edit_vote_path(vote) %></td>
        <td><%= link_to 'Destroy', vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>





<br>


<%= link_to 'New Vote', new_vote_path, class: "button" %>
</div>