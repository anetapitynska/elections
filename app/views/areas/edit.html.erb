<div id="page_wrapper">

<%= form_for([@voivodeship, @area]) do |f| %>
<div class="form-group">
	<%= f.label :name %> <br>
	<%= f.text_field :name, class: "form-control"%>
</div>

<div class="form-group">
	<%= f.label :number %> <br>
	<%= f.text_field :number, class: "form-control"%>
</div>

<div class="form-group">
	<%= f.label :people %> <br>
	<%= f.text_field :people, class: "form-control"%>
</div>

<div class="form-group">
	<%= f.label :ballots %> <br>
	<%= f.text_field :ballots, class: "form-control"%>
</div>

<div class="form-group">
	<%= f.label :empty_votes %> <br>
	<%= f.text_field :empty_votes, class: "form-control"%>
</div>
<div class="form-group">
	<%= f.label :incorrect_votes %> <br>
	<%= f.text_field :incorrect_votes, class: "form-control"%>
</div>
<p>
	<%= f.submit %>
</p>

<% end %>




</div>