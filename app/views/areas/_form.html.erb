<%= form_for([@voivodeship, @voivodeship.areas.build]) do |f| %>

<div class="form-group">
	<%= f.label 'Nazwa' %> <br>
	<%= f.text_field :name, class: "form-control"%>
</div>

<div class: "form-group">
	<%= f.label "Numer" %> <br>
	<%= f.number_field :number, class: "form-control"%> <br>
</div>

<div class: "form-group">
	<%= f.label "Ilość osób uprawnionych do głosowania" %> <br>
	<%= f.number_field :people , class: "form-control"%> <br>
</div>

<div class: "form-group">
	<%= f.label "Ilość wydanych kart wyborczych"  %> <br>
	<%= f.number_field :ballots, class: "form-control"%> <br>
</div>

<div class="form-group">
	<%= f.label "Ilość pustych głosów" %> <br>
	<%= f.number_field :empty_votes, class: "form-control"%> 
</div>

<div class: "form-group">
	<%= f.label "Ilość niepoprawnych głosów" %> <br>
	<%= f.number_field :incorrect_votes, class: "form-control"%> 
</div>

<div class: "form-group">
	<br><%= f.submit "Wyślij" %>
</div>


<% end %>

