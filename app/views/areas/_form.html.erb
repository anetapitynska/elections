<%= form_for([@voivodeship, @voivodeship.areas.build]) do |f| %>
<div class="form-group">
	<%= f.label :name %> <br>
	<%= f.text_field :name, class: "form-control"%>
</div>

<div class: "form-group">
	<%= f.label :number %> <br>
	<%= f.number_field :number, class: "form-control"%>


<div class: "form-group">
	<%= f.label :people %> <br>
	<%= f.number_field :people , class: "form-control"%>
</div>

<div class: "form-group">
	<%= f.label :ballots %> <br>
	<%= f.number_field :ballots, class: "form-control"%>
</div>


<div class="form-group">
	<%= f.label :empty_votes %> <br>
	<%= f.number_field :empty_votes , class: "form-control"%>
</div>

<div class: "form-group">
	<%= f.label :incorrect_votes %> <br>
	<%= f.number_field :incorrect_votes, class: "form-control"%>

<div class: "form-group">
	<%= f.submit %>
</div>


<% end %>

