<%= form_for @voivodeship,  html: { multipart: true } do |f| %>
	<% if @voivodeship.errors.any? %>
		<div id="errors">
			<h2><%= pluralize(@voivodeship.errors.count, "error") %> prevented this voivodeship from saving</h2>
		</div>
		<ul> 
			<% @voivodeship.errors.full_messages.each do |msg| %>
			<li> <%= msg %></li>
			<% end %>
		</ul>
	<% end %>
<div class: "form-group">
	<%= f.label :name %> <br>
	<%= f.text_field :name,  class: "form-control" %>
</div>

<div class: "form-group">
	<%= f.label :seats %> <br>
	<%= f.number_field :seats ,  class: "form-control" %>
</div>

<div class: "form-group">
	<%= f.label :image %> <br>
	<%= f.file_field :image %>
</div>

<p>
	<%= f.submit %>
</p>
<% end %>