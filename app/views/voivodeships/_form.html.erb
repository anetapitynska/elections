<%= form_for @voivodeship,  html: { multipart: true } do |f| %>
	<% if @voivodeship.errors.any? %>
		<div id="errors">
			<h2>Województwo nie zostało zapisane. <br> Błędy: <%= @voivodeship.errors.count %> </h2>
		</div>
		<ul> 
			<% @voivodeship.errors.full_messages.each do |msg| %>
			<li> <%= msg %></li>
			<% end %>
		</ul>
	<% end %>
<div class: "form-group">
	<%= f.label :name,'Nazwa' %> <br>
	<%= f.text_field :name,  class: "form-control" %>
</div>

<div class: "form-group">
	<%= f.label 'Ilość miejsc w sejmiku wojewódzkim' %> <br>
	<%= f.number_field :seats ,  class: "form-control" %>
</div>

<div class: "form-group">
	<%= f.label 'Herb' %> <br>
	<%= f.file_field :image %>
</div>

<p>
	<br>
	<%= f.submit 'Zapisz' %>
</p>
<% end %>